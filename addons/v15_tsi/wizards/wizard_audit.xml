				</group>
				<group col="4" string="General Company Information">
					<field name="partner_id"/>
					<field name="office_address"/>
					<field name="invoicing_address"/>
					<field name="telp"/>
					<field name="email"/>
					<field name="website"/>
					<field name="closing_by"/>
					<field name="category" />
					<field name="pic_crm" />
					<field name="note" />
				</group>
				<group col="4" string="Direct" attrs="{'invisible': [('closing_by', '!=', 'direct')]}">
					<field name="pic_direct" />
					<field name="transport_by" />
					<field name="email_direct" />
					<field name="tipe_klien_transport" attrs="{'invisible': [('transport_by', '!=', 'klien')]}" />
					<field name="phone_direct" />
					<field name="hotel_by" />
					<field name="show_9001" attrs="{'invisible':True}"/>
					<field name="tipe_klien_hotel" attrs="{'invisible': [('hotel_by', '!=', 'klien')]}" />
