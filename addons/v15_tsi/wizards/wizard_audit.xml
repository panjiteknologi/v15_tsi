<odoo>

	<record id="tsi_wizard_audit_view" model="ir.ui.view">
		<field name="name">Request for Audit</field>
		<field name="model">tsi.wizard_audit</field>
		<field name="arch" type="xml">
			<form>
				<group>
					<field name="cycle" />
					<field name="cycle1" widget="radio" options="{'horizontal': true}" attrs="{'invisible': [('cycle', '!=', 'cycle1')]}"/>
					<field name="cycle2" widget="radio" options="{'horizontal': true}" attrs="{'invisible': [('cycle', '!=', 'cycle2')]}"/>
					<field name="cycle3" widget="radio" options="{'horizontal': true}" attrs="{'invisible': [('cycle', '!=', 'cycle3')]}"/>
