    <record id="view_wizard_personal_information_form" model="ir.ui.view">
        <field name="name">tsi.wizard.personal.information.form</field>
        <field name="model">tsi.wizard.personal.information</field>
        <field name="arch" type="xml">
            <form string="Import Personal Information">
                <group>
                    <field name="data_type"/>
                    <field name="employee_id"/>
                    <field name="file" filename="filename"/>
                </group>
                <footer>
                    <button name="action_download_template" type="object" string="Download Template" class="btn-secondary"/>
                    <button name="action_import_file" type="object" string="Import File" class="btn-primary"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
